{
    "name": "polyfill-service",
    "description": "A polyfill as a service",
    "repository": {
        "type": "git",
        "url": "https://github.com/reiniergs/polyfill-service.git"
    },
    "bugs": {
        "url": "https://github.com/reiniergs/polyfill-service/issues"
    },
    "scripts": {
        "start": "bin/polyfill-service",
        "prepublish": "npm run build",
        "clean-dist": "rm -rf ./polyfills/__dist",
        "clean-testdata": "rm -rf ./test/results",
        "clean": "npm run clean-dist && npm run clean-testdata",
        "build": "npm run clean && node tasks/node/updatesources && node tasks/node/buildsources",
        "container-build": "docker build -t registry.heroku.com/ft-polyfill-service-qa/web .",
        "container-test": "npm run container-build && docker run -p 3000:3000 -it registry.heroku.com/ft-polyfill-service-qa/web",
        "deploy": "npm run container-build && docker push registry.heroku.com/ft-polyfill-service-qa/web && grunt deployvcl:qa && ./tasks/purge.js",
        "promote": "docker pull registry.heroku.com/ft-polyfill-service-qa/web && docker tag registry.heroku.com/ft-polyfill-service-qa/web registry.heroku.com/ft-polyfill-service/web && docker push registry.heroku.com/ft-polyfill-service/web && grunt deployvcl:prod && NODE_ENV=production ./tasks/purge.js",
        "test": "grunt test"
    },
    "license": "CC0-1.0",
    "engines": {
        "node": ">6.0.0"
    },
    "dependencies": {
        "Base64": "^1.0.0",
        "array.of": "^0.1.1",
        "audio-context-polyfill": "^1.0.0",
        "babel-core": "^6.13.2",
        "babel-preset-es2015": "^6.1.18",
        "blocked": "^1.1.0",
        "convert-source-map": "^1.3.0",
        "denodeify": "^1.2.1",
        "dotenv": "^2.0.0",
        "exists-sync": "0.0.3",
        "express": "^4.4.4",
        "fast-stats": "0.0.3",
        "graphite": "0.0.6",
        "grunt": "^1.0.1",
        "grunt-cli": "^1.2.0",
        "grunt-contrib-clean": "^1.0.0",
        "handlebars": "^4.0.5",
        "html5shiv": "^3.7.3",
        "intersection-observer": "^0.1.1",
        "intl": "^1.2.4",
        "js-polyfills": "^0.1.22",
        "json3": "^3.3.2",
        "lodash": "^4.15.0",
        "lru-cache": "^4.0.1",
        "marked": "^0.3.6",
        "measured": "^1.0.0",
        "minimist": "^1.2.0",
        "mkdirp": "^0.5.0",
        "mocha": "^3.0.2",
        "moment": "^2.8.3",
        "morgan": "^1.6.1",
        "mysql2": "^1.0.0-rc.11",
        "picturefill": "^3.0.1",
        "proclaim": "^3.4.1",
        "raven": "^0.12.1",
        "request": "^2.74.0",
        "request-promise": "^4.1.1",
        "semver": "^5.3.0",
        "tsort": "0.0.1",
        "useragent": "^2.1.9",
        "usertiming": "^0.1.8",
        "yaku": "^0.16.6",
        "mutationobserver-shim": "^0.3.2"
    },
    "devDependencies": {
        "batch": "^0.5.1",
        "fastly": "Financial-Times/fastly#v2.5.0",
        "grunt-contrib-watch": "^1.0.0",
        "grunt-shell": "^1.3.1",
        "grunt-eslint": "^19.0.0",
        "grunt-simple-mocha": "^0.4.0",
        "mkdirp": "^0.5.1",
        "node-fetch": "^1.6.0",
        "sauce-tunnel": "^2.2.3",
        "uglify-js": "^2.4.14",
        "wd": "^0.4.0"
    },
    "version": "3.12.1"
}