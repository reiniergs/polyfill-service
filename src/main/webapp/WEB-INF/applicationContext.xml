<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="baselineVersionsPath" class="java.lang.String">
        <constructor-arg type="String" value="./configs/baselineVersions.json" />
    </bean>

    <bean id="userAgentAliasesPath" class="java.lang.String">
        <constructor-arg type="String" value="./configs/userAgentAliases.json" />
    </bean>

    <bean id="aliasesPath" class="java.lang.String">
        <constructor-arg type="String" value="./polyfills/__dist/aliases.json" />
    </bean>

    <bean id="polyfillsDirPath" class="java.lang.String">
        <constructor-arg type="String" value="./polyfills/__dist/" />
    </bean>
</beans>
